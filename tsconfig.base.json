{
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "isolatedModules": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "exactOptionalPropertyTypes": false
  }
}
