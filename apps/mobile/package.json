{
  "name": "@u3/mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile application for U3-Stack",
  "main": "expo-router/entry",
  "scripts": {
    "start": "pnpm expo start -c",
    "android": "pnpm expo run:android",
    "ios": "pnpm expo run:ios",
    "build:android": "pnpm expo run:android --variant release",
    "build:ios": "pnpm expo run:ios --configuration Release",
    "prebuild": "pnpm expo prebuild",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.12",
    "@u3/ui": "workspace:*",
    "burnt": "^0.12.2",
    "expo": "~53.0.8",
    "expo-router": "~5.1.4",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "tamagui": "^1.132.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@expo/metro-config": "~0.20.0",
    "@expo/metro-runtime": "^5.0.4",
    "@tamagui/babel-plugin": "^1.132.10",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo-dev-client": "~5.2.4",
    "typescript": "~5.8.3"
  }
}
