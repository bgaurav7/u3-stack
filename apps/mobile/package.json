{
  "name": "@u3/mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile application for U3-Stack",
  "main": "expo-router/entry",
  "scripts": {
    "start": "pnpm expo start -c",
    "android": "pnpm expo run:android",
    "ios": "pnpm expo run:ios",
    "build:android": "pnpm eas build -p android --local",
    "build:ios": "pnpm eas build -p ios --local",
    "prebuild": "pnpm expo prebuild",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "clean": "rm -rf .expo .turbo .tamagui android ios node_modules",
    "validate": "pnpm typecheck && pnpm lint && pnpm format:check"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.2",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@tamagui/animations-react-native": "^1.132.12",
    "@tamagui/static": "1.132.12",
    "@u3/ui": "workspace:*",
    "burnt": "^0.12.2",
    "expo": "~53.0.8",
    "expo-router": "~5.1.4",
    "react": "^19.0.0",
    "react-native": "^0.79.2",
    "tamagui": "^1.132.12"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@expo/metro-config": "~0.20.0",
    "@expo/metro-runtime": "^5.0.4",
    "@tamagui/babel-plugin": "^1.132.12",
    "@tamagui/config-default": "1.132.12",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eas-cli": "^16.17.3",
    "expo-dev-client": "~5.2.4",
    "typescript": "~5.8.3"
  }
}
