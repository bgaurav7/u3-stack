    "tamagui": "^1.132.10",
    "@u3/ui": "workspace:*",
    "expo": "~53.0.20",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "^5.2.4",
    "expo-modules-core": "^2.5.0",
    "expo-router": "^5.1.4",
    "expo-status-bar": "~2.2.3",
    "invariant": "^2.2.4",
    "react": "^19.0.0",
    "react-native": "^0.79.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.11.1",
    "whatwg-fetch": "^3.6.20"
