{
  "name": "@u3/mobile",
  "version": "0.1.0",
  "private": true,
  "description": "Mobile application for U3-Stack",
  "main": "expo-router/entry",
  "scripts": {
    "start": "pnpm expo start -c",
    "android": "pnpm expo run:android",
    "ios": "pnpm expo run:ios",
    "build:android": "pnpm expo run:android --variant release",
    "build:ios": "pnpm expo run:ios --configuration Release",
    "prebuild": "pnpm expo prebuild",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "@tamagui/config": "^1.132.10",
    "@u3/ui": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~53.0.8",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "^0.20.0",
    "tamagui": "^1.132.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.20.0",
    "@tamagui/babel-plugin": "^1.132.10",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  }
}
