# Default environment variables for U3-Stack
# These values are used as fallbacks across all environments

# Application
NODE_ENV=development
APP_NAME=U3-Stack
APP_VERSION=0.1.0

# Server Configuration
PORT=3000
HOST=localhost

# Database (defaults for development)
DATABASE_URL=postgresql://localhost:5432/u3stack_dev

# Authentication (Clerk - these will be overridden per environment)
CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# API Configuration
API_URL=http://localhost:3000/api

# Logging
LOG_LEVEL=info

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_ERROR_TRACKING=false