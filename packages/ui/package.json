{
  "name": "@u3/ui",
  "version": "0.1.0",
  "description": "Shared UI component library for U3-Stack",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.132.10",
    "@tamagui/button": "^1.132.10",
    "@tamagui/card": "^1.132.10",
    "@tamagui/config": "^1.132.10",
    "@tamagui/core": "^1.132.10",
    "@tamagui/input": "^1.132.10",
    "@tamagui/text": "^1.132.10",
    "@tamagui/theme": "^1.132.10",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-native": "^0.73.0",
    "tsup": "^8.3.5",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-native": ">=0.79.0"
  }
}
