{
  "name": "@u3/ui",
  "version": "0.1.0",
  "description": "Shared UI component library for U3-Stack",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tamagui-build --skip-types && pnpm run build:types",
    "build:types": "tsc --project tsconfig.build.json",
    "watch": "tamagui-build --skip-types --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.132.10",
    "@tamagui/config": "^1.132.10",
    "@tamagui/font-inter": "^1.132.10",
    "@tamagui/shorthands": "^1.132.10",
    "@tamagui/themes": "^1.132.10",
    "@tamagui/toast": "^1.132.10",
    "@tamagui/web": "^1.132.10",
    "react-native-web": "^0.19.13",
    "tamagui": "^1.132.10"
  },
  "devDependencies": {
    "@tamagui/build": "^1.132.10",
    "@types/react": "^19.0.10",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-native": ">=0.79.0"
  }
}
